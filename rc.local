#!/bin/sh

iptables -A FORWARD -s 10.103.100.0/24 -j ACCEPT

iptables -A FORWARD -d 10.103.100.0/24 -j ACCEPT

iptables -t nat -A POSTROUTING -o eth0 ! -p udp -j MASQUERADE

iptables -t nat -A POSTROUTING -o eth0 -p udp -j FULLCONENAT --to-ports 10000-65535 --random-fully

iptables -t nat -A PREROUTING -i eth0 -p udp -m multiport --dports 10000:65535 -j FULLCONENAT

exit
